{
    "name": "Just the facts",
    "version": "0.0",
    "page_action": {
        "default_title": "Just the facts"
    },
    "content_scripts": [
        {
            "css": ["highlight.css"],
            "matches": [
                "*://*.nytimes.com/*",
                "*://*.economist.com/*",
                "*://*.washingtonpost.com/*",
                "*://*.wsj.com/*",
                "*://*.foxnews.com/*",
                "*://*.cnn.com/*",
                "*://*.npr.org/*",
                "*://*.huffingtonpost.com/*",
                "*://*.theguardian.com/*",
                "*://*.theguardian.co.uk/*",
                "*://*.reuters.com/*",
                "*://*.bbc.com/*",
                "*://*.bbc.co.uk/*",
                "file://*/*"
            ],
            "run_at": "document_end",
            "js": ["lib/jquery-2.0.3.min.js", "highlight.js"]
        }
    ],
    "icons": {
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "manifest_version": 2
}
